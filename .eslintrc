{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "react",
    "flowtype",
    "jest"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "camelcase": 0,
    "arrow-parens": [2, "always"]
  }
}
